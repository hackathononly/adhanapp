---
import Layout from "../layouts/Layout.astro";
import MainContent from "../components/MainContent";
import Constants from "../constants";

const state = Constants.defaultSettings.state;
const zones = Constants.defaultSettings.waktuSolatZones;

const { zone } = Astro.params;
const currentZone = zone ? zone : Constants.defaultSettings.zone;
console.log(zones, "index");
---

<Layout title={"adhan.app"} state={state} zones={zones} isIndex={true}>
	<MainContent isIndex={true} currentZone={currentZone} client:only="react" />
</Layout>
